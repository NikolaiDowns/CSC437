{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts,json --exec \"npm run build && npm run start:node\"",
    "build": "npm run clean && npx esbuild-node-tsc --outdir dist",
    "start": "npm run build && npm run start:node",
    "clean": "rm -rf dist",
    "start:node": "node dist/index.js",
    "start:proto": "cross-env STATIC=../proto/dist npm run start",
    "check": "tsc --noEmit"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "mongoose": "^8.15.0",
    "qs": "^6.14.0"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.0.2",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^22.15.18",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.4",
    "esbuild-node-tsc": "^2.0.5",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3"
  }
}
